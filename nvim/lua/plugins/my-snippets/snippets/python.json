{
		"if": {
				"prefix": "if",
				"body": [
						"if ${1:condition} :",
						"\t${2:pass}"
				],
				"description": "Code snippet \"if\""
		},
		"elif": {
				"prefix": "elif",
				"body": [
						"elif ${1:condition} :",
						"\t${2:pass}"
				],
				"description": "Code snippet \"elif\""
		},
		"else": {
				"prefix": "else",
				"body": ["else :", "\t${1:pass}"],
				"description": "Code snippet \"else\""
		},
		"while": {
				"prefix": "while",
				"body": [
						"while ${1:condition} :",
						"\t${2:pass}"
				],
				"description": "Code snippet \"while\""
		},
		"for": {
				"prefix": "for",
				"body": [
						"for ${1:i} in ${2:iterator} :",
						"\t${3:pass}"
				],
				"description": "Code snippet \"for\""
		},
		"def": {
				"prefix": "def",
				"body": [
						"def ${1:name} (${2:}) :",
						"\t${3:pass}"
				],
				"description": "Code snippet \"function\""
		},
		"deft": {
				"prefix": "deft",
				"body": [
						"def ${1:name} (${2:}) -> ${3:None} :",
						"\t${4:pass}"
				],
				"description": "Code snippet \"type function\""
		},
		"class": {
				"prefix": "class",
				"body": [
						"class ${1:name} (${2:Object}) :",
						"\tdef __init__ (self, ${3:parameters}) :",
						"\t\t${4:pass}"
				],
				"description": "Code snippet \"class\""
		},
		"with": {
				"prefix": "with",
				"body": [
						"with ${1:target} as ${2:variable} :",
						"\t${3:pass}"
				],
				"description": "Code snippet \"with\""
		},
		"try/except": {
				"prefix": "try",
				"body": [
					"try :",
					"\t${1:pass}",
					"except ${2:Exception} as ${3:e} :",
					"\t${4:pass}"
				],
				"description": "Code snippet \"try\""
		},
		"main": {
				"prefix": "ifmain",
				"body": [
					"if __name__ == \"__main__\" :",
					"\t${1:pass}"
				],
				"description": "Code snippet \"__main__\""
		},
